{
	"version": "2.0.0",
	"tasks": [
    {
      "type": "cargo",
      "subcommand": "build",
      "problemMatcher": [
        "$rustc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "label": "Rust: cargo build",
      "dependsOrder": "sequence",
      "dependsOn": [
        "Rust: cargo clean"
      ]
    },
    {
      "type": "cargo",
      "subcommand": "clean",
      "problemMatcher": [
        "$rustc"
      ],
      "group": "clean",
      "label": "Rust: cargo clean"
    },
    {
      "type": "cargo",
      "subcommand": "run",
      "problemMatcher": [
        "$rustc"
      ],
      "label": "Rust: cargo run"
    },
    {
      "type": "cargo",
      "subcommand": "test",
      "problemMatcher": [
        "$rustc"
      ],
      "group": "test",
      "label": "Rust: cargo test"
    }
  ]
}